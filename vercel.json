{
  "version": 2,
  "rewrites": [
    {
      "source": "/pdf-to-word",
      "destination": "/pdf-to-word.html"
    },
    {
      "source": "/pdf-to-word.html",
      "destination": "/pdf-to-word.html"
    },
    {
      "source": "/pdf-to-word-converter",
      "destination": "/frontend/pdf-to-word-converter.html"
    },
    {
      "source": "/pdf-to-word-converter.html",
      "destination": "/frontend/pdf-to-word-converter.html"
    },
    {
      "source": "/pdf-to-jpg",
      "destination": "/frontend/pdf-to-jpg.html"
    },
    {
      "source": "/pdf-editor",
      "destination": "/frontend/pdf-editor.html"
    },
    {
      "source": "/background-workspace",
      "destination": "/server/public/background-workspace.html"
    },
    {
      "source": "/background-workspace.html",
      "destination": "/server/public/background-workspace.html"
    },
    {
      "source": "/background-remover",
      "destination": "/server/public/background-remover.html"
    },
    {
      "source": "/background-remover.html",
      "destination": "/server/public/background-remover.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/index",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
